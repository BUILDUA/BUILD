{
    "env": {
        "NODE_ENV": "development"
    },
    "debug": {
        "active": true,
        "liveReload": [
            "/norman"
        ]
    },
    "server": {},
    "logging": {
        "logDirectory": "logs",
        "output": {
            "stdout": {
                "type": "console"
            },
            "logfile": {
                "type": "file",
                "path": "norman_{now:yyyyMMdd}.log"
            }
        },
        "loggers": {
            "*": {
                "stdout": "debug",
                "logfile": "info"
            }
        }
    },
    "db": {
        "hosts": "localhost",
        "database": "norman",
        "options": {
            "db": {
                "w": 1
            },
            "server": {
                "poolSize": 5,
                "socketOptions": {
                    "keepAlive": 1
                }
            }
        }
    },
    "deployment": {
        "strategy": "single"
    },
    "http": {
        "port": 9000,
        "proxy": {
            "host": "proxy",
            "port": 8080
        },
        "secureFlag": false
    },
    "services": "services.json",
    "web": {
        "root": "../dev",
        "errors": {
            "root": "errors"
        },
        "indexFallback": [
            "/verifyemail",
            "/auth",
            "/auth/*",
            "/login",
            "/signup",
            "/norman",
            "/forgot-password",
            "/reset-password/*"
        ],
        "compression": {
            "threshold": 512
        }
    },
    "session": {
        "secret": "3b1f6c2f75566d88034c25bf52745e0f79dc6daf4851918acd91b6dbc754e0e5"
    },
	"security": {
		"registration": {
			"self": true,
			"social": true
		},
		"provider": {
			"local": true
		}
	}

}
